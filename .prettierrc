{
    "printWidth": 120,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "importOrder": [
        "^(reflect-metadata)$",
        "<THIRD_PARTY_MODULES>",
        "^@(?!(growave-io|src)).*$",
        "^@growave-io.*$",
        "^@src.*$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderGroupNamespaceSpecifiers": false,
    "importOrderCaseInsensitive": true,
    "importOrderParserPlugins": ["typescript", "decorators-legacy"]
}
